{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch build cross-llvm",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "build",
        "cross-llvm",
        "--output-directory-path",
        "/tmp/output/cross-llvm"
      ]
    },
    {
      "name": "Launch package cross-llvm",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "package",
        "tarball",
        "-c",
        "--build-output-path",
        "/tmp/output/cross-llvm",
        "--output-file-path",
        "/tmp/package/cross-llvm.tar.gz"
      ]
    },
    {
      "name": "Launch install cross-llvm",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "install",
        "tarball",
        "-c",
        "-s",
        "/tmp/package/cross-llvm.tar.gz",
        "-D",
        "/tmp/install-test"
      ]
    },
    {
      "name": "Launch build linux-headers",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "build",
        "linux-headers",
        "--output-directory-path",
        "/tmp/output/linux-headers"
      ]
    },
    {
      "name": "Launch package linux-headers",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "package",
        "tarball",
        "-c",
        "--build-output-path",
        "/tmp/output/linux-headers",
        "--output-file-path",
        "/tmp/package/linux-headers.tar.gz"
      ]
    },
    {
      "name": "Launch install linux-headers",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "install",
        "tarball",
        "-c",
        "-s",
        "/tmp/package/linux-headers.tar.gz",
        "-D",
        "/tmp/install/linux-headers"
      ]
    }
  ]
}
